#include<iostream>
using namespace std;

void change_val(int &x, int val){
    x=val;
}

int main(){

    int arr[5]={1, 2, 3, 4, 5};
    int arr1[5]={2, 4, 6, 8, 10};

    cout<<"Initial Array: "<<endl;
    for(int i=0; i<5; i++){
    cout<<arr[i]<<" ";
    }

    for(int i=0; i<5; i++){
        change_val(arr[i], arr1[i]);
    }

    cout<<"\nChanged Array: "<<endl;
    for(int i=0; i<5; i++){
        cout<<arr[i]<<" ";
    }

    return 0;

}


/*==========OUTPUTS==========
Initial Array: 
1 2 3 4 5 
Changed Array: 
2 4 6 8 10 
*/
